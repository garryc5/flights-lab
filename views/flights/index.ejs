<%- include('header')%>
<table>
    <thead>
      <tr>
        <th>Flight Number</th>
        <th>Airport</th>
        <th>Airline</th>
        <th>Time</th>
         <th></th>
      </tr>
    </thead>
    <tbody>
      <% flights.forEach(function(f) { %>
        <tr>
          <td><%= f.flightNo %></td>
          <td><%= f.airport %></td>
          <td><%= f.airline %></td>
          <td><%= f.departs.toLocaleDateString() %></td>
          <td><a href="/flights/<%= f._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
    <a href="/flights/new">add a flight</a>
    <%- include('footer')%>